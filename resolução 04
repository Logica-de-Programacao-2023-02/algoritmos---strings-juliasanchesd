package main

import "fmt"

func main() {

	var x1, x2 string

	fmt.Print("Digite a primeira string: ")
	fmt.Scan(&x1)

	fmt.Print("Digite a segunda string: ")
	fmt.Scan(&x2)

	if x1 == x2 {
		fmt.Println("As duas strings são iguais!")
	} else {
		fmt.Println("As duas strings são diferentes!")
	}
}
